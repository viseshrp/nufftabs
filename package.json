{
  "name": "wxt-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:e2e": "wxt build -c tests/e2e/wxt.e2e.config.ts -m e2e",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "quality": "pnpm compile",
    "smoke": "pnpm build",
    "smoke:e2e": "pnpm test:e2e",
    "package": "pnpm zip",
    "postinstall": "wxt prepare",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "pnpm build:e2e && playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.4",
    "jsdom": "^24.1.3",
    "typescript": "^5.9.3",
    "vitest": "^2.1.4",
    "wxt": "^0.20.6"
  }
}
